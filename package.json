{
  "name": "prettysize",
  "description": "Convert bytes to other sizes for prettier logging",
  "version": "0.1.0",
  "main": "./index.js",
  "devDependencies": {
    "codecov": "^2.3.0",
    "istanbul": "~0.1.8",
    "jshint": "~0.9.0",
    "vows": "*",
    "yui-lint": "~0.1.1"
  },
  "keywords": [
